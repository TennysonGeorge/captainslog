<%= form_for(extractor, :url => book_extractor_path(extractor.book.slug, extractor)) do |f| %>
  <%= render "form/field", :f => f, :field => :label %>
  <%= render "form/field", :f => f, :field => :match %>
  <%= render "form/field", :f => f, :field => :type %>
  <%= render "form/actions", :f => f, :back_url => edit_book_path(extractor.book.slug) %>
<% end %>
