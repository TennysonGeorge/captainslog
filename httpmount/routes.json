{
  "service.UserService": {
    "/sso": {
      "POST": {
        "handler": "GenerateToken",
        "request": "service.UserLoginRequest",
        "response": "service.UserToken"
      }
    }
  },
  "service.ReportService": {
    "/api/reports": {
      "GET": {
        "handler": "Retrieve",
        "request": "service.ReportRetrieveRequest",
        "response": "service.ReportRetrieveResponse"
      }
    }
  },
  "service.BookService": {
    "/api/books": {
      "POST": {
        "handler": "Create",
        "request": "service.BookCreateRequest",
        "response": "model.Book"
      },
      "GET": {
        "handler": "Retrieve",
        "request": "service.BookRetrieveRequest",
        "response": "service.BookRetrieveResponse"
      }
    }
  },
  "service.SavedQueryService": {
    "/api/saved_query": {
      "POST": {
        "handler": "Create",
        "request": "service.SavedQueryCreateRequest",
        "response": "model.SavedQuery"
      },
      "PUT": {
        "handler": "Update",
        "request": "model.SavedQuery",
        "response": "model.SavedQuery"
      },
      "GET": {
        "handler": "Retrieve",
        "response": "service.SavedQueriesRetrieveResponse"
      }
    }
  },
  "service.ExtractorService": {
    "/api/extractors": {
      "POST": {
        "handler": "Create",
        "request": "service.ExtractorCreateRequest",
        "response": "model.Extractor"
      }
    }
  },
  "service.EntryService": {
    "/api/entries/search": {
      "GET": {
        "handler": "Search",
        "request": "service.EntrySearchRequest",
        "response": "service.EntrySearchResponse"
      }
    }
  },
  "service.QueryService": {
    "/api/query": {
      "GET": {
        "handler": "Schema",
        "response": "service.Schema"
      },
      "POST": {
        "handler": "Query",
        "request": "service.QueryExecuteRequest",
        "response": "service.QueryResults"
      }
    }
  },
  "service.ShorthandService": {
    "/api/shorthands": {
      "POST": {
        "handler": "Create",
        "request": "service.ShorthandCreateRequest",
        "response": "model.Shorthand"
      }
    }
  }
}
