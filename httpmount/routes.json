{
  "routes": [
    {
      "endpoint": "/api/books",
      "service": "service.BookService",
      "methods": [
        {
          "method": "POST",
          "handler": "Create",
          "request": "service.BookCreateRequest",
          "response": "model.Book"
        },
        {
          "method": "GET",
          "handler": "Retrieve",
          "response": "service.BookRetrieveResponse"
        }
      ]
    },
    {
      "endpoint": "/api/extractors",
      "service": "service.ExtractorService",
      "methods": [
        {
          "method": "POST",
          "handler": "Create",
          "request": "service.ExtractorCreateRequest",
          "response": "model.Extractor"
        }
      ]
    },
    {
      "endpoint": "/api/entries",
      "service": "service.EntryService",
      "methods": [
        {
          "method": "POST",
          "handler": "Create",
          "request": "service.EntryCreateRequest",
          "response": "service.EntryCreateResponse"
        },
        {
          "method": "GET",
          "handler": "Retrieve",
          "response": "service.EntryRetrieveResponse"
        }
      ]
    },
    {
      "endpoint": "/api/query",
      "service": "service.QueryService",
      "methods": [
        {
          "method": "POST",
          "handler": "Query",
          "request": "service.QueryRequest",
          "response": "service.QueryResponse"
        }
      ]
    },
    {
      "endpoint": "/api/shorthands",
      "service": "service.ShorthandService",
      "methods": [
        {
          "method": "POST",
          "handler": "Create",
          "request": "service.ShorthandCreateRequest",
          "response": "model.Shorthand"
        }
      ]
    }
  ]
}
