{
  "routes": [
    {
      "endpoint": "/api/book",
      "service": "BookService",
      "methods": [
        {
          "method": "POST",
          "handler": "Create",
          "request": "BookCreateRequest",
          "response": "model.Book"
        },
        {
          "method": "GET",
          "handler": "Retrieve",
          "response": "BookRetrieveResponse"
        }
      ]
    },
    {
      "endpoint": "/api/extractor",
      "service": "ExtractorService",
      "methods": [
        {
          "method": "POST",
          "handler": "Create",
          "request": "ExtractorCreateRequest",
          "response": "model.Extractor"
        }
      ]
    },
    {
      "endpoint": "/api/entry",
      "service": "EntryService",
      "methods": [
        {
          "method": "POST",
          "handler": "Create",
          "request": "EntryCreateRequest",
          "response": "EntryCreateResponse"
        },
        {
          "method": "GET",
          "handler": "Retrieve",
          "response": "EntryRetrieveResponse"
        }
      ]
    }
  ]
}
