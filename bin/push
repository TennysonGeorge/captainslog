#!/bin/bash

images=(minond/captainslog-puller-web
        minond/captainslog-puller-worker
        minond/captainslog-web
        minond/captainslog-worker
        minond/captainslog-processor
        minond/captainslog-querier)

for image in ${images[@]}; do
  echo "pushing $image ..."
done

for image in ${images[@]}; do
  docker push "$image:latest" &> /dev/null &
done

wait
echo "done pushing ${#images[@]} images"
